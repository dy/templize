{
  "name": "templize",
  "version": "1.3.0",
  "description": "Template parts for elements",
  "bugs": {
    "url": "https://github.com/spectjs/templize/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spectjs/templize.git"
  },
  "license": "MIT",
  "author": "Dmitry Iv.",
  "keywords": [
    "template-parts",
    "template-instantiation",
    "template instantiation",
    "template-instance",
    "template instance",
    "dom-parts",
    "dom parts",
    "templatize"
  ],
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "browser": "templize.js",
  "files": [
    "src",
    "templize.js",
    "templize.min.js"
  ],
  "scripts": {
    "test": "node -r ./test/register.cjs test/index.js",
    "build": "rollup ./src/index.js --file templize.js --format esm",
    "minify": "terser templize.js -o templize.min.js --module -c passes=3 -m"
  },
  "devDependencies": {
    "hyperf": "^1.0.1",
    "jsdom": "^19.0.0",
    "rollup": "^2.63.0",
    "spect": "^23.1.1",
    "terser": "^5.10.0",
    "tst": "^7.0.0",
    "value-ref": "^1.2.1",
    "wait-please": "^3.1.0"
  },
  "dependencies": {
    "element-props": "^2.0.2",
    "sube": "^1.0.2",
    "subscript": "^6.2.0",
    "swapdom": "^1.1.0"
  }
}
